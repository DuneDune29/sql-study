select deptno, sum(sal), avg(sal) from emp
  2  group by deptno;

    DEPTNO   SUM(SAL)   AVG(SAL)
---------- ---------- ----------
        30       9400 1566.66667
        20      10875       2175
        10       8750 2916.66667
        
        
        
