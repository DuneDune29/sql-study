select deptno, avg(sal) from emp
  2  group by deptno
  3  having avg(sal) >= 2000;

    DEPTNO   AVG(SAL)
---------- ----------
        20       2175
        10 2916.66667



select deptno, avg(sal) from emp
  2  group by deptno;

    DEPTNO   AVG(SAL)
---------- ----------
        30 1566.66667
        20       2175
        10 2916.66667



select deptno, max(sal), min(sal)
  2  from emp
  3  group by deptno
  4  having max(sal) > 2900;

    DEPTNO   MAX(SAL)   MIN(SAL)
---------- ---------- ----------
        20       3000        800
        10       5000       1300



select deptno, avg(sal) from emp
  2  where sal >= 1000
  3  group by deptno
  4  having avg(sal) >= 2000;

    DEPTNO   AVG(SAL)
---------- ----------
        20    2518.75
        10 2916.66667



select job, deptno, sum(sal), count(*) from emp
  2  where deptno = 20
  3  group by job, deptno;

JOB                    DEPTNO   SUM(SAL)   COUNT(*)
------------------ ---------- ---------- ----------
MANAGER                    20       2975          1
ANALYST                    20       6000          2
CLERK                      20       1900          2



select deptno, avg(sal), sum(sal), count(*) from emp
  2  group by deptno
  3  having count(*) >= 5;

    DEPTNO   AVG(SAL)   SUM(SAL)   COUNT(*)
---------- ---------- ---------- ----------
        30 1566.66667       9400          6
        20       2175      10875          5



       
       
        
        
        
        

      
      
      
      

      
      
