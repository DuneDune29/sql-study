select deptno as No, dname as Name from dept;

NO NAME
---------- ----------------------------
        10 ACCOUNTING
        20 RESEARCH
        30 SALES
        40 OPERATIONS
        10 ACCOUNTING
        20 RESEARCH
        30 SALES
        40 OPERATIONS
        
    
